{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla and NodeJS",
  "author": "Hyungwon Jin",
  "dependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^10.0.4",
    "aws-sdk": "^2.850.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.2.0",
    "mini-css-extract-plugin": "^1.3.2",
    "mongoose": "^5.10.18",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss": "^8.1.14",
    "postcss-loader": "^4.1.0",
    "pug": "^3.0.0",
    "routes": "^2.1.0",
    "sass-loader": "^10.1.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node init.js",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development && webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production && webpack",
    "build:server": "babel src --out-dir build --ignore src/static --ignore src/assets --ignore src/webpack.config.js",
    "copyAll": "xcopy src\\static build\\static /d /e /h /c /y /i && xcopy src\\views build\\views /d /e /h /c /y /i",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rd /s \"build\""
  },
  "devDependencies": {},
  "browserslist": [
    "defaults"
  ]
}